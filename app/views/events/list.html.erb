<h1>Camping Schedule</h1>
<% for club in @clubs %>
	<% unless club.events.empty? %>	
		<h2><%=h club.club_name %></h2>
<table>
  <tr>
    <th>Start date</th>
    <th>End date</th>
    <th>Location</th>
    <th>Location city</th>
    <th>Location state</th>
  </tr>
		<% for event in club.events %>
	    <tr>
        <td><%=h event.start_date %></td>
        <td><%=h event.end_date %></td>
        <td><%=h event.location %></td>
        <td><%=h event.location_city %></td>
        <td><%=h event.location_state %></td>
        <td><%= link_to 'Show', event %></td>
      </tr>
    <% end %>
  <% end %>
</table>  
<% end %>






