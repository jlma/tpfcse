<h1>Listing clubs</h1>

<table>
  <tr>
    <th>Club name</th>
    <th>State</th>
    <th>Logo file</th>
  </tr>

<% for club in @clubs %>
  <tr>
    <td><%=h club.club_name %></td>
    <td><%=h club.state %></td>
    <td><%=h club.logo_file %></td>
    <td><%= link_to 'Show', club %></td>
    <td><%= link_to 'Edit', edit_club_path(club) %></td>
    <td><%= link_to 'Destroy', club, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New club', new_club_path %>
